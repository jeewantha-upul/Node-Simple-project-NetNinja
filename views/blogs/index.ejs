 <html lang="en">

  <%- include('../partials/head') %>

 <body>
 
  <%- include('../partials/nav') %>

   <div class="blogs content">
     <h2>All blogs</h2>
    
     <% if(blogs.length > 0){ %>
     
      <% blogs.forEach( blog=>{ %>
        <a href="/blogs/<%=blog._id%>">
          <h3><%= blog.title %></h3>
          <h3><%= blog.snippet %></h3>
        </a>
        
         
       <% })}  %>
      
          
      
   </div>

   <%- include('../partials/footer') %>
 </body>
 </html>

  